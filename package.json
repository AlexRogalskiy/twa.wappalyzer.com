    "deploy:v2": "ENVIRONMENT=v2 AWS_PROFILE=wappalyzer nuxt generate && aws s3 sync dist s3://twa.wappalyzer.com --acl public-read --cache-control max-age=31536000,public"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.1",
