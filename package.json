{
  "name": "Wappalyzer",
  "version": "1.0.0",
  "description": "Identify technologies on websites.",
  "author": "Wappalyzer",
  "scripts": {
    "dev": "ENVIRONMENT=beta nuxt",
    "prod": "NODE_ENV=production ENVIRONMENT=v2 nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
